{
  "main": "cosy/public/index.php",
  "output": "dist/cosy.phar",
  "chmod": "0755",
  "stub": true,
  "git-version": "package_version",  
  "shebang": "#! /usr/bin/env php",
  "finder": [
    {
      "name": "*",
      "in": "cosy"
    },
    {
      "name": "*.php",
      "in": "vendor",
      "exclude": [
        "herrera-io",
        "kherge",
        "phpseclib",
        "yaml"
      ]
    }
  ],
  "directories": [
    "cosy"
  ],
  "files": [
    "README.md",
    "LICENSE"
  ],
  "algorithm": "SHA1",
  "compression": "GZ"
}
